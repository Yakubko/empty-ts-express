{
    "name": "empty-ts-express",
    "version": "1.0.0",
    "description": "Empty ts express app",
    "main": "dist/server.js",
    "scripts": {
        "start": "tsc && node dist/server.js",
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' -r dotenv/config server.ts",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Yakubko/empty-ts-express.git"
    },
    "keywords": [],
    "author": "Jakub Miskech <yakubmiskech@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Yakubko/empty-ts-express/issues"
    },
    "homepage": "https://github.com/Yakubko/empty-ts-express#readme",
    "dependencies": {
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/jest": "^25.2.3",
        "@types/node": "^14.0.5",
        "@types/supertest": "^2.0.9",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
        "coveralls": "^3.1.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^26.0.1",
        "nodemon": "^2.0.4",
        "prettier": "^2.0.5",
        "supertest": "^4.0.2",
        "ts-jest": "^26.0.0",
        "ts-node": "^8.10.1",
        "typescript": "^3.9.3"
    },
    "jest": {
        "testEnvironment": "node",
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        }
    }
}
